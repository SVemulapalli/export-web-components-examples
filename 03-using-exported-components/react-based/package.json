{
  "name": "groceries",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "vue": "^2.6.10"
  },
  "scripts": {
    "copy:prereqs": "npm run copy:vendor && npm run copy:built:output",
    "copy:built:output": "rm -rf ./public/exported && mkdir ./public/exported && cp -R ../../02-export-examples/built-output/ ./public/exported",
    "copy:vendor": "rm -rf ./public/vendor && mkdir ./public/vendor && cp node_modules/vue/dist/vue.min.js ./public/vendor/",
    "prestart": "npm run copy:prereqs",
    "start": "react-scripts start",
    "prebuild": "npm run copy:prereqs",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
